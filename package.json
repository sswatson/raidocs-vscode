{
  "name": "raidocs",
  "displayName": "raidocs",
  "description": "Tools for authoring in the RAI documentation system",
  "publisher": "sswatson",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/sswatson/raidocs-vscode"
  },
  "version": "0.1.13",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "raidocs.runPythonCodeBlock",
        "title": "RelationalAI Docs: Run Python Code Block"
      },
      {
        "command": "raidocs.restartKernel",
        "title": "RelationalAI Docs: Restart Kernel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run prepackage",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "prepackage": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "package": "vsce package --no-dependencies",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "publish": "vsce publish --no-dependencies",
    "vsce": "vsce"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@jupyterlab/services": "^7.3.4",
    "@vscode/debugadapter": "^1.68.0",
    "@vscode/vsce": "^3.2.1"
  }
}
